
import java.util.Scanner;

public class ExericioModulo1 {

	public static void main(String[] args) {
		// Exerc√≠cio 1
		Scanner sc = new Scanner(System.in);
		int num1, num2, resultado;
		num1 = sc.nextInt();
		num2 = sc.nextInt();	
		resultado = num1 + num2;
		System.out.printf("SOMA = %d%n",resultado);
		sc.close();
		
	}

}
